{
  "rewrites": [
    {
      "source": "/artists/:id", 
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": "(facebookexternalhit|Twitterbot|LinkedInBot|WhatsApp|Pinterest|Slackbot|TelegramBot|Google.*snippet)"
        }
      ],
      "destination": "/api/ssr/artists/:id"
    }
  ]
}
