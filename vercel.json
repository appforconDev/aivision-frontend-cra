
{
  "rewrites": [
    {
      "source": "/artists/:id+",         
      
      "destination": "/api/ssr/artists/:id"
    }
  ],
  "headers": [
    {
      "source": "/api/ssr/artists/:id",
      "headers": [
        { "key": "Cache-Control", "value": "s-maxage=3600, stale-while-revalidate" }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "aivisioncontest.com" }],
      "destination": "https://www.aivisioncontest.com/:path*",
      "permanent": true
    }
  ]
}
