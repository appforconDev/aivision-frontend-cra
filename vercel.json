{
  "rewrites": [
    {
      "source": "/artist/:id*",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": "(?i)(facebookexternalhit|twitterbot|pinterest|linkedinbot|whatsapp|slackbot)"
        }
      ],
      "destination": "/api/ssr/artists/:id*"
    },
    {
      "source": "/artists/:id*",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": "(?i)(facebookexternalhit|twitterbot|pinterest|linkedinbot|whatsapp|slackbot)"
        }
      ],
      "destination": "/api/ssr/artists/:id*"
    }
  ],
  "headers": [
    {
      "source": "/api/ssr/artists/:id*",
      "headers": [
        { "key": "Cache-Control", "value": "s-maxage=3600, stale-while-revalidate" }
      ]
    }
  ]
}